{
  "name": "@dex-toolkit/utils",
  "version": "1.0.0",
  "author": "",
  "license": "ISC",
  "keywords": [
    "dex-toolkit",
    "ethereum",
    "blockchain",
    "uniswap",
    "dex"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/esm/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "start": "tsc -p tsconfig.esm.json --watch",
    "gen:abis": "abi-types-generator generate"
  },
  "dependencies": {
    "@dex-toolkit/number": "workspace:*",
    "@ethereum-multicall/core": "workspace:*",
    "@ethereum-multicall/utils": "workspace:*",
    "@ethersproject/providers": "5.7.2",
    "bignumber.js": "9.1.2",
    "node-fetch": "2",
    "ethers": "5.7.2"
  },
  "devDependencies": {
    "@dex-toolkit/types": "workspace:*",
    "@ethereum-abi-types-generator/converter-typescript": "workspace:*",
    "@ethereum-abi-types-generator/core": "workspace:*",
    "@ethereum-multicall/types": "workspace:*",
    "@types/node-fetch": "2.6.11"
  }
}
